   "source": [
    "### Counting the number of bits set to one in an integer\n",
    "Input: an integer\\\n",
    "Output: Number of bits set to one"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "id": "5053dc3a-ce0e-4b34-a639-f7e7fe1d2bad",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "3\n"
     ]
    }
   ],
   "source": [
    "def count_bits(x):\n",
    "    num = 0\n",
    "    while(x):\n",
    "        num += x & 1\n",
    "        x >>= 1\n",
    "    return num\n",
    "\n",
    "print(count_bits(7))"
   ]
  },
